<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.harsh24.wastemanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/waste"
        android:label="Waste Management"
        android:roundIcon="@mipmap/waste"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main2" />
        <activity android:name=".ActivitySignin" />
        <activity android:name=".ActivitySignup" />
        <activity android:name=".AppMainActivity"></activity>
    </application>

</manifest>